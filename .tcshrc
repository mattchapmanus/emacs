#
# Make Some awk Aliases
# ---------------------
#
@ i = 1
set dlr = '$'
while ($i < 20)
 set name = "awk$i"
 alias $name "awk '{ print $dlr$i }'"
 set pnl = 'print "\n"'
 set psr  = 'printf "%s "'
 set name = "awk${i}nf"
 alias $name "awk '{ for (n = $i; n <= NF; n++) $psr, ${dlr}n; $pnl }'"
 @ i += 1
end
set NF = NF
alias awknf "awk '{ print $dlr$NF }'"
unset name NF

